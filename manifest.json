{
  "name": "thisIDEAsucks",
  "version": "0.0.1",
  "manifest_version": 2,
  "description" : "TODO",
  "permissions": [
      "storage"
  ],

  "browser_action": {
    "default_icon": {
      "19": "icons/19x19.png",
      "38": "icons/38x38.png"
    },
    "default_title": "thisIDEAsucks",
    "default_popup": "browseraction/popup.html"
  },

  "background": {
      "scripts": ["background.js"],
      "persistent": false
  },

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "<all_urls>"],
      "js": ["jquery-1.11.1.min.js","content.js"],
      "all_frames": true
    }
  ]
}
